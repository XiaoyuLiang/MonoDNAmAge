\name{MonoFunc}
\alias{MonoFunc}
\title{Monocyte DNA Methylation Age}
\usage{
MonoFunc(input,mean.trans=F,linear.trans=F)
}
\description{
Estimate Monocyte DNA Methylation Age
}
\arguments{
   \item{input}{dataframe or matrix: DNA methylation matrix (CpG by Sample), rownames are the names of CpGs}
   \item{mean.trans}{logical: if \code{TRUE}, Estimation will be transfered}
   \item{linear.trans}{logical: if \code{TRUE}, Estimation will be transfered}
}
